void InsertSort(int array[],int length)

{

for(int i =0 ;i<length;i++)

{
     int temp = array[i];

     for(int j =i-1;j>=0;j--)

     {

           if(array[j]>temp)

           {

                    array[j+1] = array[j];

           }

           else

           {

                    break;

           }

     }

     array[j+1] = temp;

        

}

}
